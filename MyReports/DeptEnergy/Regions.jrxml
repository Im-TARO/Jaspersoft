	<queryString language="SQL">
		<![CDATA[WITH prices_2023 AS
  (SELECT s.region,
          e.year,
          e.month,
          avg(e.residential_price) residential_price,
          avg(e.commercial_price) commercial_price ,
          avg(e.industrial_price) industrial_price,
          avg(e.transportation_price) transportation_price
   FROM deptofenergy.elec_monthly_sales e
   JOIN codes.us_states s ON e.state = s.state_code
   WHERE e.year = 2023
   group by region, year, month
   ),
prices_2024 AS
  (SELECT s.region,
          e.year,
          e.month,
          avg(e.residential_price) residential_price,
          avg(e.commercial_price) commercial_price ,
          avg(e.industrial_price) industrial_price,
          avg(e.transportation_price) transportation_price
   FROM deptofenergy.elec_monthly_sales e
   JOIN codes.us_states s ON e.state = s.state_code
   WHERE e.year = 2024
   group by region, year, month
   )
SELECT 
    a.region,
    a.year a_year,
    a.month,
    a.residential_price a_residential_price,
    a.commercial_price a_commercial_price,
    a.industrial_price a_industrial_price,
    a.transportation_price a_transportation_price,   
	 b.year b_year,
    b.residential_price b_residential_price,
    b.commercial_price b_commercial_price,
    b.industrial_price b_industrial_price,
    b.transportation_price b_transportation_price
FROM
    prices_2024 a
    JOIN prices_2023 b on a.region = b.region AND a.month = b.month
ORDER BY CASE
             WHEN a.region = 'Northeast' THEN 1
             ELSE a.region
         END,
         month]]>
	</queryString>
